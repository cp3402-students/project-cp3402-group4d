			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--wix-madefor-display)",
					"fontSize": "0.9rem"
				}
			},
			"core/group": {
				"color": {
					"background": "#ececec"
				}
			},
			"core/paragraph": {
				"typography": {
					"lineHeight": "1.2"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--custom-backgroud)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--custom-color-4)",
					"text": "var(--wp--preset--color--custom-color-8)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--custom-color-6)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--custom-color-7)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--wix-madefor-display)",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.8",
					"textTransform": "capitalize"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--custom-color-9)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--custom-color-3)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--wix-madefor-display)",
					"fontSize": "18px",
					"fontStyle": "normal",
					"fontWeight": "500",
					"textDecoration": "none"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--wix-madefor-display)",
			"fontSize": "18px",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
